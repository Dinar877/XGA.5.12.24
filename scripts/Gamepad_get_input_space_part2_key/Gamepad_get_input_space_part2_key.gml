function Gamepad_get_input_space_part2_key() {
	if (gp_input != gp_input_old)
	{
		gp_input_temp = gp_input_old
			
		//if textblock was space
		if (gp_input_old = global.key_space)
		{
				Gamepad_get_input_key3()
					
				exit
		}
		else if (gp_input_old = global.key_ability)
		{
			Gamepad_get_input_key3()
					
			exit
		}
		else if (gp_input_old = global.key_tab)
		{
			Gamepad_get_input_key3()
				
			exit
		}
		else if (gp_input_old = global.key_G)
		{
			Gamepad_get_input_key3()
					
			exit
		}
		else if (gp_input_old = global.key_select)
		{
			Gamepad_get_input_key3()
					
				exit
		}
		else if (gp_input_old = global.key_enter)
		{
			Gamepad_get_input_key3()
					
				exit
		}
		else if (gp_input_old = global.key_L1)
		{
			Gamepad_get_input_key3()
					
				exit
		}
		else if (gp_input_old = global.key_L2)
		{
			Gamepad_get_input_key3()
					
				exit
		}
		else if (gp_input_old = global.key_R1)
		{
			Gamepad_get_input_key3()
					
				exit
		}
		else if (gp_input_old = global.key_R2)
		{
			Gamepad_get_input_key3()
					
				exit;
		}
	}


}
